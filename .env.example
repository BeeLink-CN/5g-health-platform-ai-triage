# Environment Configuration

# NATS Configuration
NATS_URL=nats://localhost:4222
NATS_STREAM=events
NATS_DURABLE=ai-triage

# Contracts Path (JSON Schemas)
CONTRACTS_PATH=./contracts

# Rules Configuration
RULES_PATH=./rules/default.json

# State Management
# TTL for patient state cache in milliseconds (default: 10 minutes)
STATE_TTL_MS=600000

# HTTP Server
HTTP_PORT=8092

# Logging
LOG_LEVEL=info
